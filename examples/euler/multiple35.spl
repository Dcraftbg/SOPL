extern "C" printf(*char, int : int);
func main() {
    let i:   int = 0;
    let sum: int = 0;
    while i < 1000 {
        if i % 3 == 0 {
            sum += i;
        }
        else {
            if i % 5 == 0 {
                sum += i;
            }
        }
        i += 1;
    }
    printf("Sum: %d\n", sum);
}
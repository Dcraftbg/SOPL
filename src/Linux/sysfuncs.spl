include "./syscalls.spl"

func sread(fd: int, buf: ptr, count: long) {
    RAX = SYS_read
    RDI = fd
    RSI = buf
    RDX = count
    interrupt I_SYSCALL
}
func swrite(fd: int, buf: ptr, count: long) {
    RAX = SYS_write
    RDI = fd
    RSI = buf
    RDX = count
    interrupt I_SYSCALL
}
